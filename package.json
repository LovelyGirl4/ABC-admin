{
  "name": "g-will-admin",
  "version": "1.0.0",
  "description": "g-will后台管理",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development PORT=8080 webpack-dev-server",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run clean && cross-env NODE_ENV=production webpack -p --progress --colors",
    "build-test": "npm run clean && cross-env NODE_ENV=production DEPLOY_ENV=test webpack -p --progress --colors",
    "test": "jest",
    "test:watch": "jest --watch",
    "deploy": "npm run lint && npm run build && cross-env DEPLOY_ENV=production node ./tools/deploy.js && npm run clean",
    "deploy-test": "npm run lint && npm run build-test && cross-env DEPLOY_ENV=test node ./tools/deploy.js && npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git@git.coding.net:bb_rd/G-will.git"
  },
  "author": "Zhichao Zhang",
  "license": "MIT",
  "dependencies": {
    "antd": "^2.12.7",
    "echarts": "^3.7.2",
    "file-saver": "^1.3.3",
    "isomorphic-fetch": "^2.2.1",
    "ramda": "^0.24.1",
    "react": "^15.5.4",
    "react-cropper": "^1.0.0",
    "react-dom": "^15.5.4",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.8",
    "reselect": "^3.0.1",
    "xlsx": "^0.11.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "postcss-loader": "^1.3.3",
    "react-test-renderer": "^15.5.4",
    "redux-logger": "^3.0.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4"
  },
  "vendor": [
    "echarts",
    "react",
    "react-dom",
    "react-router-dom",
    "react-redux",
    "redux",
